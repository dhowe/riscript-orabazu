{
  "name": "tsup-library-template",
  "version": "0.0.1",
  "url": "https://github.com/orabazu/tsup-libary-template",
  "scripts": {
    "ts-types": "tsc",
    "build:dev": "rm -rf lib/ && tsup",
    "build:prod": "rm -rf dist/ && env NODE_ENV=production tsup",
    "build:watch": "tsup --watch"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "tsup": "^6.0.1",
    "typescript": "^4.7.2"
  },
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "files": [
    "lib",
    "dist"
  ]
}
